{
  "name": "react-visual-diff",
  "version": "0.3.8",
  "description": "A react component that can render a diff of two react components",
  "author": "juliankrispel",
  "license": "MIT",
  "repository": "Tettra/react-visual-diff",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c -m",
    "start": "rollup -c -w -m",
